<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <include
        latin:keyboardLayout="@xml/rowkeys_qwerty3" />

    <!-- U+00F6: "รถ" LATIN SMALL LETTER O WITH DIAERESIS
         U+00F3: "รณ" LATIN SMALL LETTER O WITH ACUTE -->
    <Key
        latin:keySpec="&#x00F6;"
        latin:keyHintLabel="&#x00F3;"
        latin:moreKeys="!text/morekeys_o" />

    <!-- U+00E7: "รง" LATIN SMALL LETTER C WITH CEDILLA
         U+0107: "ฤ" LATIN SMALL LETTER C WITH ACUTE -->
    <Key
        latin:keySpec="&#x00E7;"
        latin:keyHintLabel="&#x0107;"
        latin:additionalMoreKeys="!text/morekeys_c" />
</merge>